{
  "name": "lifecycle",
  "version": "1.0.0",
  "main": "dist/bundle.cjs.js",
  "module": "dist/bundle.esm.js",
  "sideEffects": false,
  "repository": "https://github.com/kokiebisu/build-lifecycle.git",
  "author": "Kenichi Okiebisu <a01056715@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-babel": "^5.3.0",
    "eslint": "^7.20.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "husky": "4.2.3",
    "lint-staged": "^10.5.4",
    "prettier-eslint": "^12.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "rollup": "^2.39.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "scripts": {
    "start": "node lib/index.js",
    "lint": "eslint **/*.js",
    "lint:fix": "prettier-eslint $PWD/'**/*.js' --write",
    "prebuild": "rimraf dist",
    "build": "rollup --config rollup.config.js --environment NODE_ENV:production",
    "serve": "rollup --config rollup.config.js --environment NODE_ENV:development"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn lint:fix"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.12.17",
    "@babel/preset-env": "^7.12.17",
    "@babel/preset-react": "^7.12.13",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "eslint-plugin-react": "^7.22.0",
    "lifecycle": "^1.0.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rollup-plugin-string": "^3.0.0",
    "serve": "^11.3.2"
  },
  "peerDependencies": {
    "react": ">=15"
  }
}
